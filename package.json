{
  "name": "data-quality-app",
  "version": "31.0.0",
  "private": true,
  "devDependencies": {
    "@dhis2/d2-i18n-generate": "^1.0.19",
    "chai": "^4.1.2",
    "cucumber": "^5.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-dhis2": "^3.0.7",
    "jest-enzyme": "^7.0.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-cucumber-framework": "^2.2.8",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^5.6.4"
  },
  "dependencies": {
    "@dhis2/d2-i18n": "^1.0.3",
    "@dhis2/d2-ui-header-bar": "^5.2.6",
    "autoprefixer": "9.4.9",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "^7.0.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "classnames": "^2.2.5",
    "css-loader": "2.1.0",
    "d2": "^31.4.0",
    "d2-manifest": "^1.0.0",
    "d2-ui": "^29.0.18",
    "d2-utilizr": "^0.2.15",
    "d3-color": "^1.0.3",
    "dotenv": "6.2.0",
    "eslint": "5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "3.2.0",
    "import-export": "^1.0.1",
    "jest": "24.1.0",
    "loglevel": "^1.6.0",
    "material-ui": "^0.20.0",
    "npm": "^6.8.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "prop-types": "^15.6.0",
    "raf": "3.4.1",
    "react": "^16.2.0",
    "react-dev-utils": "^7.0.3",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "rxjs": "^6.4.0",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "manifest": "d2-manifest package.json ./public/manifest.webapp",
    "start": "yarn manifest && yarn localize && node scripts/start.js",
    "build": "yarn manifest && yarn localize && node scripts/build.js && cp ./package.json ./build/package.json",
    "localize": "d2-i18n-generate -n data-quality-app -p ./i18n/ -o ./src/locales/",
    "test": "node scripts/test.js --env=jsdom",
    "test-e2e": "wdio ./e2e/config/wdio.conf.js",
    "validate": "yarn list",
    "lint": "yarn localize && eslint src/",
    "coverage": "yarn test --coverage",
    "extract-pot": "node scripts/pot-extractor.js",
    "refresh-translations-template": "yarn extract-pot -o en.pot && git add ./i18n/en.pot"
  },
  "pre-commit": [
    "lint",
    "validate",
    "refresh-translations-template"
  ],
  "manifest.webapp": {
    "name": "DHIS2 Data Quality App",
    "description": "DHIS2 Data Quality App",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "https://dhis2.github.io",
      "name": "DHIS2"
    },
    "dhis2": {
      "apiVersion": "29"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  },
  "homepage": ".",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
